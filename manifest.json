{
	"manifest_version":2,
	"name":"Grepme",
	"version":"0.1",

	"description":"Extension to save bookmarks and search paths for projects",
	// "icons":{
	// 	"19":"",
	// 	"38":"",
	// },

	"browser_action":{
		"default_icon":{
		"19":"icon19.png",
		"38":"icon38.png"
		},
		"default_title":"Grep Me",
		"default_popup":"popup.html"
	},

	"background":{
		"scripts":["background.js"],
		"persistent":false
	},

	"content_scripts":[
		{
			"matches":["<all_urls>"],
			"js":["jquery-2.1.4.min.js","content.js"]
		}
	],

	"permissions":[
		"activeTab",
		"tabs"
	]

	// "web_accessible_resources":[

	// ],



}